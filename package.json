{
    "name": "pizzashop",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start:client": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "start:server": "nodemon -e ts -w ./ -x npm run server:dev",
        "server:dev": "ts-node --esm ./server/app.ts",
        "dev": "concurrently -n client,server \"npm run start:client\" \"npm run start:server\"",
        "preview": "vite preview",
        "start": "ts-node --esm ./server/app.ts",
        "heroku-start": "ts-node --esm \"./server/app.ts\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install  && npm run build "
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.14",
        "@mui/material": "^5.14.14",
        "@types/crypto-js": "^4.1.3",
        "@types/uuid": "^9.0.6",
        "cors": "^2.8.5",
        "crypto-js": "^4.1.1",
        "express": "^4.18.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.15",
        "@types/express": "^4.17.20",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "concurrently": "^8.2.2",
        "eslint": "^8.45.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "rollup-plugin-node-builtins": "^2.1.2",
        "ts-node-dev": "^2.0.0",
        "vite": "^4.4.5"
    }
}
